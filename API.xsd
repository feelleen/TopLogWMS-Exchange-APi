<xs:schema xmlns:tns="http://ws.toplogwms.ru/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ws.toplogwms.ru/" attributeFormDefault="unqualified" elementFormDefault="qualified">
	<xs:complexType name="AcceptanceOrder">
		<xs:complexContent>
			<xs:extension base="tns:OrderLink">
				<xs:choice>
					<xs:element name="Owner" type="tns:Owner" minOccurs="0"/>
					<xs:element name="Provider" type="tns:Provider" minOccurs="0"/>
					<xs:element name="CargoUnits" type="tns:CargoUnit" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Goods" type="tns:Good" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="OrderRows" type="tns:AcceptanceOrderRow" maxOccurs="unbounded"/>
					<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
				<xs:attribute name="ArrivalDate" type="xs:date"/>
				<xs:attribute name="AcceptanceDate" type="xs:date"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AcceptanceOrderRow">
		<xs:choice>
			<xs:element name="Reservation" type="tns:Reservation" minOccurs="0"/>
			<xs:element name="Batch" type="tns:Batch" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="OrderRowID" type="xs:string"/>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="GoodName" type="xs:string"/>
		<xs:attribute name="Quantity" type="xs:decimal" use="required"/>
		<xs:attribute name="Quality" type="xs:integer"/>
		<xs:attribute name="CrossQuantity" type="xs:decimal"/>
		<xs:attribute name="CargoID" type="xs:string"/>
		<xs:attribute name="Price" type="xs:decimal"/>
		<xs:attribute name="InsurancePrice" type="xs:decimal"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
		<xs:attribute name="KeepingVariantID" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AccountTag">
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="AddProperty">
		<xs:attribute name="Key" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
		<xs:attribute name="Value" use="required"/>
	</xs:complexType>
	<xs:complexType name="Address">
		<xs:attribute name="PlainAddress" type="xs:string"/>
		<xs:attribute name="PostAddress" type="xs:string"/>
		<xs:attribute name="PickpointID" type="xs:string"/>
		<xs:attribute name="Zip" type="xs:string"/>
		<xs:attribute name="Region" type="xs:string"/>
		<xs:attribute name="Area" type="xs:string"/>
		<xs:attribute name="City" type="xs:string"/>
		<xs:attribute name="Street" type="xs:string"/>
		<xs:attribute name="House" type="xs:string"/>
		<xs:attribute name="Flat" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AdviceDoc">
		<xs:attribute name="DocID" type="xs:string" use="required"/>
		<xs:attribute name="DocNum" type="xs:int" use="required"/>
		<xs:attribute name="DocDate" type="xs:dateTime" use="required"/>
		<xs:attribute name="Storeman" type="xs:string"/>
		<xs:attribute name="Comment" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="AdviceRow">
		<xs:choice>
			<xs:element name="Variant" type="tns:KeepingVariant" minOccurs="0"/>
			<xs:element name="Reservation" type="tns:Reservation" minOccurs="0"/>
			<xs:element name="Batch" type="tns:Batch" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="GoodName" type="xs:string"/>
		<xs:attribute name="Quantity" type="xs:decimal" use="required"/>
		<xs:attribute name="QuantityVar" type="xs:decimal"/>
		<xs:attribute name="CargoID" type="xs:string"/>
		<xs:attribute name="OrderRowID" type="xs:string"/>
		<xs:attribute name="QualityID" type="xs:integer"/>
		<xs:attribute name="ProblemID" type="xs:integer"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
		<xs:attribute name="OrderID" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BalanceRow">
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="GoodName" type="xs:string"/>
		<xs:attribute name="StockID" type="xs:int" use="required"/>
		<xs:attribute name="OwnerID" type="xs:int" use="required"/>
		<xs:attribute name="Balance" type="xs:decimal"/>
		<xs:attribute name="Diff" type="xs:decimal"/>
		<xs:attribute name="Reserved" type="xs:decimal"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="BarCode">
		<xs:attribute name="Action" type="xs:int" use="required"/>
		<xs:attribute name="BarCode" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Batch">
		<xs:choice>
			<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="SerialRange" type="tns:Range" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="SerialID" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="Description" type="xs:string" use="required"/>
		<xs:attribute name="ReceiptDate" type="xs:dateTime"/>
		<xs:attribute name="ProductionDate" type="xs:dateTime"/>
		<xs:attribute name="LastCheckDate" type="xs:dateTime"/>
		<xs:attribute name="BestBeforeDay" type="xs:dateTime"/>
		<xs:attribute name="UseBeforeDate" type="xs:dateTime"/>
		<xs:attribute name="SerialNumber" type="xs:string"/>
		<xs:attribute name="AdditionalInf" type="xs:string"/>
		<xs:attribute name="StatusID" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="Brand">
		<xs:attribute name="BrandID" type="xs:string" use="required"/>
		<xs:attribute name="BrandName" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="BundleContent">
		<xs:choice>
			<xs:element name="bundleFeature" type="tns:Feature" minOccurs="0"/>
			<xs:element name="contentGood" type="tns:Good"/>
			<xs:element name="contentFeature" type="tns:Feature" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="bundleSKUbarcode" type="xs:string"/>
		<xs:attribute name="contentSKUbarcode" type="xs:string"/>
		<xs:attribute name="Quantity" type="xs:decimal" use="required"/>
	</xs:complexType>
	<xs:complexType name="CargoUnit">
		<xs:attribute name="InvoiceNum" type="xs:string"/>
		<xs:attribute name="InvoiceDate" type="xs:date"/>
		<xs:attribute name="CargoID" type="xs:string"/>
		<xs:attribute name="UnitsQnt" type="xs:integer" default="1"/>
		<xs:attribute name="Weight" type="xs:decimal" default="0"/>
		<xs:attribute name="Volume" type="xs:decimal" default="0"/>
		<xs:attribute name="Price" type="xs:decimal" default="0"/>
		<xs:attribute name="InsurancePrice" type="xs:decimal" default="0"/>
	</xs:complexType>
	<xs:complexType name="Channel">
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Client">
		<xs:choice>
			<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="ClientID" type="xs:string" use="required"/>
		<xs:attribute name="ClientName" type="xs:string" use="required"/>
		<xs:attribute name="Phone" type="xs:string"/>
		<xs:attribute name="Email" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ClientReturnClaim">
		<xs:complexContent>
			<xs:extension base="tns:OrderLink">
				<xs:choice>
					<xs:element name="ClaimRows" type="tns:ClientReturnClaimRow" maxOccurs="unbounded"/>
					<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
				<xs:attribute name="StockID" type="xs:integer"/>
				<xs:attribute name="OwnerID" type="xs:integer"/>
				<xs:attribute name="ClientID" type="xs:string"/>
				<xs:attribute name="CauseID" type="xs:integer"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ClientReturnClaimRow">
		<xs:choice>
			<xs:element name="Order" type="tns:OrderLink" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="ClaimRowID" type="xs:string"/>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="GoodName" type="xs:string"/>
		<xs:attribute name="Quantity" type="xs:decimal" use="required"/>
		<xs:attribute name="Quality" type="xs:integer"/>
		<xs:attribute name="Price" type="xs:decimal"/>
		<xs:attribute name="InsurancePrice" type="xs:decimal"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
		<xs:attribute name="KeepingVariantID" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="Contractor">
		<xs:choice>
			<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="ContractorID" type="xs:string" use="required"/>
		<xs:attribute name="ContractorName" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="DeliveryInfo">
		<xs:choice>
			<xs:element name="Route" type="tns:Route" minOccurs="0"/>
			<xs:element name="Client" type="tns:Client" minOccurs="0"/>
			<xs:element name="Contractor" type="tns:Contractor" minOccurs="0"/>
			<xs:element name="Address" type="tns:Address" minOccurs="0"/>
			<xs:element name="Comment" type="xs:string" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="DeliveryType" type="xs:integer"/>
		<xs:attribute name="RouteDate" type="xs:dateTime"/>
		<xs:attribute name="Direction" type="xs:string"/>
		<xs:attribute name="Delivery" type="xs:string"/>
		<xs:attribute name="DocumentControl" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="DeliveryOptions"/>
	<xs:complexType name="DifferenceRow">
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
		<xs:attribute name="Diff" type="xs:decimal" use="required"/>
		<xs:attribute name="StockID" type="xs:int" use="required"/>
		<xs:attribute name="AddressID" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Dimensions">
		<xs:attribute name="Height" type="xs:decimal"/>
		<xs:attribute name="Width" type="xs:decimal"/>
		<xs:attribute name="Weight" type="xs:decimal"/>
		<xs:attribute name="Length" type="xs:decimal"/>
		<xs:attribute name="Capacity" type="xs:decimal"/>
	</xs:complexType>
	<xs:complexType name="Driver">
		<xs:attribute name="DriverID" type="xs:string" use="required"/>
		<xs:attribute name="DriverName" type="xs:string" use="required"/>
		<xs:attribute name="BarCode" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Error">
		<xs:attribute name="ErrorCode" type="xs:int" use="required"/>
		<xs:attribute name="ErrorDesc" type="xs:string" use="required"/>
		<xs:attribute name="ObjectID" type="xs:string"/>
		<xs:attribute name="ErrorDetails" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Feature">
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
		<xs:attribute name="Article" type="xs:string" use="required"/>
		<xs:attribute name="Description" type="xs:string"/>
		<xs:attribute name="DeletionMark" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="Good">
		<xs:choice>
			<xs:element name="Group" type="tns:Group" minOccurs="0"/>
			<xs:element name="Brand" type="tns:Brand" minOccurs="0"/>
			<xs:element name="AccountTag" type="tns:AccountTag" minOccurs="0"/>
			<xs:element name="KeepingVariants" type="tns:KeepingVariant" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="BarCodes" type="tns:BarCode" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TechDocuments" type="tns:TechDocument" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Features" type="tns:Feature" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="BundleContent" type="tns:BundleContent" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="OpInstruction" type="tns:OperationalInstruction" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
		<xs:attribute name="Article" type="xs:string" use="required"/>
		<xs:attribute name="GoodType" type="xs:int" default="1"/>
		<xs:attribute name="BaseMeasure" type="xs:int" default="796"/>
		<xs:attribute name="FullName" type="xs:string"/>
		<xs:attribute name="SellerArticle" type="xs:string"/>
		<xs:attribute name="ERPCode" type="xs:string"/>
		<xs:attribute name="ShelfLife" type="xs:int"/>
		<xs:attribute name="StackOrder" type="xs:int"/>
		<xs:attribute name="IsBundle" type="xs:boolean" default="false"/>
		<xs:attribute name="IsMeasurable" type="xs:boolean"/>
		<xs:attribute name="Size" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="GoodDimensions">
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="Height" type="xs:decimal"/>
		<xs:attribute name="Length" type="xs:decimal"/>
		<xs:attribute name="Width" type="xs:decimal"/>
		<xs:attribute name="Weight" type="xs:decimal"/>
	</xs:complexType>
	<xs:complexType name="Group">
		<xs:choice>
			<xs:element name="Parent" type="tns:Group" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="KeepingVariant">
		<xs:choice>
			<xs:element name="Dimensions" type="tns:Dimensions" minOccurs="0"/>
			<xs:element name="Barcode" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="BarCodes" type="tns:BarCode" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="ID" type="xs:string" use="required"/>
		<xs:attribute name="Measure" type="xs:int" use="required"/>
		<xs:attribute name="KeepingVariantType" type="xs:int" use="required"/>
		<xs:attribute name="Coefficient" type="xs:decimal" use="required"/>
		<xs:attribute name="Description" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="KitOrder">
		<xs:complexContent>
			<xs:extension base="tns:OrderLink">
				<xs:choice>
					<xs:element name="Owner" type="tns:Owner" minOccurs="0"/>
					<xs:element name="OrderRows" type="tns:KitOrderRow" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="KitOrderRow">
		<xs:attribute name="OrderRowID" type="xs:string" use="required"/>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="Quantity" type="xs:decimal" use="required"/>
	</xs:complexType>
	<xs:complexType name="OperationalInstruction">
		<xs:attribute name="Operation" type="xs:int" use="required"/>
		<xs:attribute name="Instruction" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="OrderLink">
		<xs:choice>
			<xs:element name="Stock" type="tns:Stock" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="OrderID" type="xs:string" use="required"/>
		<xs:attribute name="OrderNum" type="xs:string" use="required"/>
		<xs:attribute name="OrderDate" type="xs:dateTime" use="required"/>
		<xs:attribute name="DocTypeID" type="xs:int" use="required"/>
		<xs:attribute name="ChanID" type="xs:string"/>
		<xs:attribute name="DocNum" type="xs:string"/>
		<xs:attribute name="Manager" type="xs:string"/>
		<xs:attribute name="Priority" type="xs:integer"/>
		<xs:attribute name="Comment" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="OrderStatus">
		<xs:all>
			<xs:element name="Order" type="tns:OrderLink"/>
		</xs:all>
		<xs:attribute name="StatusID" type="xs:integer" use="required"/>
		<xs:attribute name="StatusDate" type="xs:dateTime" use="required"/>
	</xs:complexType>
	<xs:complexType name="Owner">
		<xs:attribute name="OwnerID" type="xs:int" use="required"/>
		<xs:attribute name="OwnerName" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Provider">
		<xs:attribute name="ProviderID" type="xs:string" use="required"/>
		<xs:attribute name="ProviderName" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="Range">
		<xs:attribute name="RangeStart" type="xs:string" use="required"/>
		<xs:attribute name="RangeFinish" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="ReceivingAdvice">
		<xs:choice>
			<xs:element name="Order" type="tns:OrderLink"/>
			<xs:element name="Doc" type="tns:AdviceDoc" minOccurs="0"/>
			<xs:element name="AdviceRows" type="tns:AdviceRow" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="Stage" type="xs:integer"/>
	</xs:complexType>
	<xs:complexType name="Requirement">
		<xs:attribute name="Key" type="xs:string" use="required"/>
		<xs:attribute name="Name" type="xs:string" use="required"/>
		<xs:attribute name="Value" use="required"/>
	</xs:complexType>
	<xs:complexType name="Reservation">
		<xs:attribute name="ReservationID" type="xs:string" use="required"/>
		<xs:attribute name="ReservationLevel" type="xs:integer" use="required"/>
	</xs:complexType>
	<xs:complexType name="ReservationAdvice">
		<xs:choice>
			<xs:element name="Order" type="tns:OrderLink"/>
			<xs:element name="Doc" type="tns:AdviceDoc" minOccurs="0"/>
			<xs:element name="AdviceRows" type="tns:AdviceRow" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="Response">
		<xs:choice>
			<xs:element name="Errors" type="tns:Error" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="Status" type="xs:int" use="required"/>
	</xs:complexType>
	<xs:complexType name="Route">
		<xs:attribute name="RouteID" type="xs:string" use="required"/>
		<xs:attribute name="RouteName" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="SKU">
		<xs:choice>
			<xs:element name="Dimensions" type="tns:Dimensions" minOccurs="0"/>
		</xs:choice>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
		<xs:attribute name="VariantID" type="xs:string"/>
		<xs:attribute name="Quality" type="xs:int"/>
		<xs:attribute name="Barcode" type="xs:string"/>
		<xs:attribute name="Description" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="ShipmentAdvice">
		<xs:choice>
			<xs:element name="Order" type="tns:OrderLink"/>
			<xs:element name="Doc" type="tns:AdviceDoc" minOccurs="0"/>
			<xs:element name="CargoUnits" type="tns:CargoUnit" maxOccurs="unbounded"/>
			<xs:element name="AdviceRows" type="tns:AdviceRow" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="DiffRows" type="tns:AdviceRow" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="TripDeparture" type="tns:TripDeparture" minOccurs="0"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="ShipmentOrder">
		<xs:complexContent>
			<xs:extension base="tns:OrderLink">
				<xs:choice>
					<xs:element name="OrderRows" type="tns:ShipmentOrderRow" maxOccurs="unbounded"/>
					<xs:element name="DeliveryInfo" type="tns:DeliveryInfo"/>
					<xs:element name="DeliveryOptions" type="tns:DeliveryOptions" minOccurs="0"/>
					<xs:element name="AddProperties" type="tns:AddProperty" minOccurs="0" maxOccurs="unbounded"/>
				</xs:choice>
				<xs:attribute name="ShipmentDate" type="xs:dateTime"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ShipmentOrderRow">
		<xs:choice>
			<xs:element name="DeliveryInfo" type="tns:DeliveryInfo" minOccurs="0"/>
			<xs:element name="Requirements" type="tns:Requirement" minOccurs="0" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="OrderRowID" type="xs:string"/>
		<xs:attribute name="GoodID" type="xs:string" use="required"/>
		<xs:attribute name="GoodName" type="xs:string"/>
		<xs:attribute name="Quantity" type="xs:decimal" use="required"/>
		<xs:attribute name="ShipmentDate" type="xs:date"/>
		<xs:attribute name="Prelimenary" type="xs:boolean"/>
		<xs:attribute name="StockID" type="xs:int"/>
		<xs:attribute name="OwnerID" type="xs:integer"/>
		<xs:attribute name="QualityID" type="xs:integer"/>
		<xs:attribute name="Price" type="xs:decimal"/>
		<xs:attribute name="InsurancePrice" type="xs:decimal"/>
		<xs:attribute name="ShipmentGroup" type="xs:integer"/>
		<xs:attribute name="FeatureID" type="xs:string"/>
		<xs:attribute name="KeepingVariantID" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="StatusSKU">
		<xs:attribute name="SKUID" type="xs:string" use="required"/>
		<xs:attribute name="StatusID" type="xs:int" use="required"/>
	</xs:complexType>
	<xs:complexType name="Stock">
		<xs:attribute name="StockID" type="xs:string" use="required"/>
		<xs:attribute name="StockName" type="xs:string" use="required"/>
	</xs:complexType>
	<xs:complexType name="StockAdjustment">
		<xs:choice>
			<xs:element name="Stock" type="tns:Stock" minOccurs="0"/>
			<xs:element name="AdjustmentRows" type="tns:StockAdjustmentRow" maxOccurs="unbounded"/>
		</xs:choice>
		<xs:attribute name="DocID" type="xs:string" use="required"/>
		<xs:attribute name="AdjustmentType" type="xs:int" use="required"/>
